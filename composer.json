{
  "name": "dusan/php-mvc-database",
  "license": "GPL-2.0",
  "minimum-stability": "stable",
  "description": "Database package for PhpMvc framework",
  "type": "library",
  "authors": [
    {
      "name": "Dusan Malusev",
      "email": "dusan.998@outlook.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Dusan\\MicroORM\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Dusan\\MicroORM\\Tests": "tests/"
    }
  },
  "require": {
    "php": ">=7.3.0",
    "psr/container": "^1.0",
    "dusan/php-mvc-collections": "^1.0",
    "ext-pdo": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "nesbot/carbon": "^2.19",
    "ext-openssl": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "8.1.*"
  },
  "repositories": [
    {
      "branch": "master",
      "type": "git",
      "url": "https://github.com/malusev998/PhpMvc-Collections.git"
    }
  ]
}
